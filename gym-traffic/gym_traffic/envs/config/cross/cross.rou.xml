<routes>
    <vType accel="1.0" decel="5.0" id="Car" length="4.5" maxSpeed="25.0" sigma="0.5" />
    <route id="route_ns" edges="n_0 0_s"/>
    <route id="route_sn" edges="s_0 0_n"/>
    <route id="route_ew" edges="e_0 0_w"/>
    <route id="route_we" edges="w_0 0_e"/>

    <route id="route_wn" edges="w_0 0_n"/>
    <route id="route_en" edges="e_0 0_n"/>

    <flow depart="1" id="flow_n_s" route="route_ns" type="Car" begin="0" end="3600" probability="$ns" />
<!--     <flow depart="1" id="flow_s_n" route="route_sn" type="Car" begin="0" end="3600" probability="$sn" guiShape="passenger/sedan"/> -->
    <flow depart="1" id="flow_e_w" route="route_ew" type="Car" begin="0" end="3600" probability="$ew" />
    <flow depart="1" id="flow_w_e" route="route_we" type="Car" begin="0" end="3600" probability="$we" />

    <flow depart="1" id="flow_w_n" route="route_wn" type="Car" begin="0" end="3600" probability="$wn" />
    <flow depart="1" id="flow_e_n" route="route_en" type="Car" begin="0" end="3600" probability="$en" />

</routes>
